{
  "manifest_version": 2,

  "name": "Harlem Shake",
  "description": "One click party.",
  "version": "1.0",
  "icons": {
    "256": "img/icon_256.png"
  },

  "browser_action": {
    "default_icon": "img/icon_256.png"
  },
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "permissions": [
    "activeTab", "tabs", "http://*/*", "https://*/*"
  ]
}
